{
  "manifest_version": 3,
  "name": "dan-chrome-etc",
  "description": "Chrome extension for anything",
  "version": "0.1",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [],
  "permissions": ["scripting", "activeTab", "webNavigation", "tabs"],
  "host_permissions": ["http://wormhole:7117/*", "https://temporaltechnologies.slack.com/*", "https://temporalio.slack.com/*"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}
